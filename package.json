    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.2",
    "classnames": "^2.5.1",
    "deep-equal": "^1.1.2",
    "file-saver": "^2.0.5",
    "higlass": "git+https://alexpreynolds@github.com/alexpreynolds/higlass.git#apr-PR-develop-v1p13p4",
    "higlass-multivec": "git+https://alexpreynolds@github.com/alexpreynolds/higlass-multivec.git#apr-PR-master-v0p3p3",
    "higlass-register": "^0.3.0",
    "higlass-tabix-datafetcher": "git+https://alexpreynolds@github.com/alexpreynolds/higlass-tabix-datafetcher.git#apr-PR-main-v0p1p0",
