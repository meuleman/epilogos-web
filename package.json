{
  "name": "epilogos-web",
  "version": "1.2.0",
  "author": "Alex Reynolds <alexpreynolds@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/meuleman/epilogos-web"
  },
  "private": true,
  "keywords": [
    "epilogos",
    "epigenome",
    "epigenetics",
    "chromatin-modeling"
  ],
  "dependencies": {
